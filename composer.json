{
    "name": "andrey/filament-medialibrary",
    "description": "Media library for Filament 5 with visual file manager",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrey",
            "email": "andrey@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^5.0",
        "laravel/framework": "^12.0",
        "spatie/laravel-medialibrary": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Andrey\\FilamentMediaLibrary\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Andrey\\FilamentMediaLibrary\\FilamentMediaLibraryServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
